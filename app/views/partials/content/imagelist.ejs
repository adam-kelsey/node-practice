<article class="display">
  <h1 class="display-title">Artwork</h1>
  <p class="display-body">Here is all the wonderful artwork that has been submitted!</p>
  <div class="display artwork">
    <% if (artwork.length > 0) { %>
    <% for(i=0; i < artwork.length; i++) { %>
      <img src="/images/artwork/<%= artwork[i]%>" alt="Artwork <%= i %>">
    <% } %>
    <% } %>
  </div>
</article>